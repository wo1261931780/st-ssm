<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns="http://www.springframework.org/schema/beans"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	<bean id="xmlDemoDao005" class="com.stssm.github.io.junw.bbb003spring20220801.dao.impl.DemoDao005Impl">
		<property name="age" value="555"/>
		<property name="address" value="555"/>
	</bean>
	<bean id="xmlDemoDao006" class="com.stssm.github.io.junw.bbb003spring20220801.dao.impl.DemoDao005Impl">
		<property name="age" value="666"/>
		<property name="address" value="666"/>
	</bean>
	<bean id="xmlDemoService004" class="com.stssm.github.io.junw.bbb003spring20220801.service.impl.DemoService004Impl"
	      autowire="byName">
		<!--如果是按照名称装配，就允许多个配置文件指向同一个bean对象-->
		<!--要求不同的bean之间的id不同-->
		<!--其次要求上面的id和service中创建出来的对象名称一样-->
		<!--就算是按照名称装配，也必须有set方法的存在-->
	</bean>
	<!--目前不建议使用按照名称装配-->
	<!--因为按照名称装配，会出现名称和文件耦合的现象-->
	<!--还是那句话，低耦合，高内聚-->
	<!--我们的自动装配，默认只能操作引用类型-->
	<!--数据传递无法使用-->
</beans>
