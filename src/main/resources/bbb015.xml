<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns="http://www.springframework.org/schema/beans"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	<bean id="xmlDemoDao006" class="com.stssm.github.io.junw.bbb003spring20220801.dao.Impl.DemoDao006Impl">
		<!--这里，因为所有的集合类型，都是dao的内部属性-->
		<!--所以，在这里设置，全部都是作为properties存放在dao的bean中-->
		<property name="dao006Array">
			<array>
				<value>111</value>
				<value>111</value>
				<value>111</value>
			</array>
		</property>
		<property name="dao006List">
			<list>
				<!--这里array和list的标签可以混合使用-->
				<value>list222</value>
				<value>list222</value>
				<value>list222</value>
			</list>
		</property>
		<!--其实这种格式，一般用不到-->
		<!--主要是在初始化框架的时候使用这种集合-->
		<property name="dao006Set">
			<set>
				<value>set333</value>
				<value>set333</value>
				<!--由于set的特性，这里会自动过滤掉重复的内容-->
				<value>set333</value>
			</set>
		</property>
		<property name="dao006Map">
			<map>
				<entry key="map444" value="map444"/>
				<entry key="map444" value="map444"/>
				<!--map的特性，重复内容也会被过滤掉-->
				<entry key="map1444" value="map444"/>
			</map>
		</property>
		<property name="dao006Properties">
			<props>
				<prop key="name">111</prop>
				<prop key="add">222</prop>
				<prop key="age">333</prop>
				<!--这里重复的内容也被过滤掉了-->
				<prop key="name">123</prop>
			</props>
		</property>
	</bean>
	<!-- dao006Array=[111, 111, 111],-->
	<!-- dao006List=[list222, list222, list222],-->
	<!-- dao006Map={map444=map444, map1444=map444},-->
	<!-- dao006Set=[set333],-->
	<!-- dao006Properties={add=222, name=123, age=333}-->
</beans>
