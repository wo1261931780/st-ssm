<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns="http://www.springframework.org/schema/beans"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	<bean id="xmlUserDao002" class="com.stssm.github.io.junw.bbb002spring20220731.dao.Impl.UserDao002Impl"/>
	<bean id="xmlUserService002" class="com.stssm.github.io.junw.bbb002spring20220731.service.Impl.UserService002Impl"
	      init-method="initTest1" destroy-method="destoryTest1">
		<property name="diUserDao002" ref="xmlUserDao002"/>
	</bean>
	<!--上面是IoC和DI的过程-->
	<!--这里实现了初始化的接口，所以可以根据是否初始化，来判断是否存在对应过程-->
	<bean id="xmlFactory0731" class="com.stssm.github.io.junw.bbb002spring20220731.factory.Factory0731"/>
	<bean id="xmlFactory" factory-bean="xmlFactory0731" factory-method="factoryGetDao001"/>
	<!--实例工厂-->
	<bean id="staticFactory0731" factory-method="factoryGetDao002"
	      class="com.stssm.github.io.junw.bbb002spring20220731.staticFactory.StaticFactory0731"/>
	<!--静态工厂-->
</beans>
