<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns="http://www.springframework.org/schema/beans"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	<bean id="xmlDemoDao003" class="com.stssm.github.io.junw.bbb003spring20220801.dao.impl.DemoDao003Impl">
		<!--其他地方和普通配置一样-->
		<!--因为我们的简单类型变量在Dao中，所以就在Dao中配置对应的参数-->
		<property name="age" value="123"/>
		<!--这里的配置是没有顺序的，因为dao中的变量本身就没有顺序，只要能按照标准去加载传递数据就可以-->
		<property name="address" value="123"/>

	</bean>
	<bean id="xmlDemoService002" class="com.stssm.github.io.junw.bbb003spring20220801.service.impl.DemoService002Impl">
		<property name="demoDao003" ref="xmlDemoDao003"/>
	</bean>

</beans>
